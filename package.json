  "licensesUrl": "https://github.com/Alexiush/UAddMd/blob/main/LICENSE.md",
  "dependencies": {
    "com.unity.addressables": "1.21.20"
  },
  "keywords": [
    "Markdown",
    "Addressables"
  ],
  "author": {
    "name": "Alexiush",
    "email": "streltsov2609@gmail.com",
    "url": "https://github.com/Alexiush"
  },
  "samples": [
    {
      "displayName": "Base Markdown document",
      "description": "Big Markdown document showcasing how basic markdown features handled default",
      "path": "Samples~/BaseDocument"
    },
    {
      "displayName": "Tutorial with illustrations",
      "description": "Example of adressables rendering support",
      "path": "Samples~/TutorialExample"
    },
    {
      "displayName": "EditorWindow Example",
      "description": "Example on how to use MarkdownRenderer within an editor UI",
